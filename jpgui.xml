            <fileset dir="${dir.jarfile}/bin"/>
            <zipfileset excludes="META-INF/*.SF" src="H:/Apps/inifile8-2.0.23.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="C:/javafx-sdk-22.0.2/lib/javafx.base.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="C:/javafx-sdk-22.0.2/lib/javafx.controls.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="C:/javafx-sdk-22.0.2/lib/javafx.fxml.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="C:/javafx-sdk-22.0.2/lib/javafx.graphics.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="C:/javafx-sdk-22.0.2/lib/javafx.media.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="C:/javafx-sdk-22.0.2/lib/javafx.swing.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="C:/javafx-sdk-22.0.2/lib/javafx.web.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="C:/javafx-sdk-22.0.2/lib/javafx-swt.jar"/>
        </jar>
